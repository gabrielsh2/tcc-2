{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": "src",
    "module": "CommonJS",
    "target": "ESNext",
    "jsx": "react-native",
    "paths": {
      "@components": ["components/index"],
      "@screens": ["screens/index"],
      "@constants": ["constants/index"],
      "@services": ["services/index"],
      "@providers": ["providers/index"],
      "@utils": ["utils/index"]
    }
  },
  "exclude": ["node_modules"]
}
